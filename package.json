{
	"name": "@devprotocol/clubs-core",
	"version": "0.6.2-alpha.12",
	"description": "Core library for Clubs",
	"main": "dist/index.cjs",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./ui/react": {
			"import": "./ui/react/index.mjs",
			"require": "./ui/react/index.cjs"
		},
		"./ui/svelte": {
			"import": "./ui/svelte/index.mjs",
			"require": "./ui/svelte/index.cjs"
		},
		"./ui/vue": {
			"import": "./ui/vue/index.mjs",
			"require": "./ui/vue/index.cjs"
		},
		"./ui/webcomponents": {
			"import": "./ui/webcomponents/index.mjs",
			"require": "./ui/webcomponents/index.cjs"
		},
		"./connection": {
			"import": "./connection/index.mjs",
			"require": "./connection/index.cjs"
		}
	},
	"types": "./dist/clubs-core.d.ts",
	"files": [
		"dist/*.mjs",
		"dist/*.cjs",
		"dist/*.d.ts",
		"ui/**/*.d.ts",
		"ui/**/*.mjs",
		"ui/**/*.cjs",
		"connection/*.d.ts",
		"connection/*.mjs",
		"connection/*.cjs",
		"!**/*.test.*",
		"src"
	],
	"scripts": {
		"test": "ava",
		"preview": "run-p preview:tsc preview:rollup preview:astro",
		"preview:astro": "astro --root ui-preview/ dev",
		"preview:tsc": "yarn build:ts:ui --watch",
		"preview:rollup": "yarn build:rollup:ui -w",
		"build": "yarn build:ts && yarn build:ts:ui && yarn build:rollup:ui && yarn build:rollup",
		"build:ts": "tsc",
		"build:ts:ui": "tsc -p tsconfig.ui.json",
		"build:rollup": "rollup -c",
		"build:rollup:ui": "rollup -c rollup.config.ui.js",
		"watch": "tsc -w",
		"pretest": "yarn prebuild && tsc -p tsconfig.test.json",
		"prebuild": "rimraf dist ui connection",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,json,md,yml,astro}'",
		"prepack": "yarn test && yarn build",
		"prepare": "husky install"
	},
	"author": "aggre",
	"license": "MIT",
	"dependencies": {
		"@devprotocol/elements": "0.1.1",
		"@devprotocol/util-ts": "2.2.1",
		"js-base64": "^3.7.2",
		"lit": "^2.3.1",
		"ramda": "^0.28.0",
		"yaml": "^2.1.1"
	},
	"devDependencies": {
		"@astrojs/lit": "1.0.1",
		"@astrojs/react": "1.1.1",
		"@astrojs/svelte": "1.0.0",
		"@astrojs/tailwind": "1.0.0",
		"@astrojs/vue": "1.0.1",
		"@ava/typescript": "3.0.1",
		"@rollup/plugin-commonjs": "22.0.2",
		"@rollup/plugin-multi-entry": "4.1.0",
		"@rollup/plugin-typescript": "8.5.0",
		"@rollup/pluginutils": "4.2.1",
		"@types/get-installed-path": "4.0.1",
		"@types/node": "16.11.58",
		"@types/ramda": "0.28.15",
		"@types/react": "18.0.18",
		"@types/react-dom": "18.0.6",
		"@typescript-eslint/eslint-plugin": "5.36.2",
		"@typescript-eslint/parser": "5.36.2",
		"@webcomponents/template-shadowroot": "0.1.0",
		"astro": "1.1.7",
		"autoprefixer": "10.4.8",
		"ava": "4.3.3",
		"eslint": "8.23.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-functional": "4.2.2",
		"glob": "8.0.3",
		"husky": "8.0.1",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.16",
		"prettier": "2.7.1",
		"prettier-plugin-astro": "0.5.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rimraf": "3.0.2",
		"rollup": "2.79.0",
		"rollup-plugin-dts": "4.2.2",
		"svelte": "3.50.1",
		"tailwindcss": "3.1.8",
		"typescript": "4.8.3",
		"vue": "3.2.39"
	},
	"peerDependencies": {
		"astro": "^1.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"svelte": "^3.49.0",
		"vue": "^3.2.0"
	},
	"optionalDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"svelte": "^3.49.0",
		"vue": "^3.2.0"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dev-protocol/clubs-core.git"
	},
	"keywords": [],
	"bugs": {
		"url": "https://github.com/dev-protocol/clubs-core/issues"
	},
	"homepage": "https://github.com/dev-protocol/clubs-core#readme"
}
