{
	"name": "@devprotocol/clubs-core",
	"version": "0.9.4",
	"description": "Core library for Clubs",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"type": "module",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/clubs-core.d.ts"
		},
		"./ui/react": {
			"default": "./dist/src/ui/react/index.js",
			"types": "./dist/src/ui/react/index.d.ts"
		},
		"./ui/svelte": {
			"default": "./dist/src/ui/svelte/index.js",
			"types": "./dist/src/ui/svelte/index.d.ts"
		},
		"./ui/vue": {
			"default": "./dist/src/ui/vue/index.js",
			"types": "./dist/src/ui/vue/index.d.ts"
		},
		"./ui/webcomponents": {
			"default": "./dist/src/ui/webcomponents/index.js",
			"types": "./dist/src/ui/webcomponents/index.d.ts"
		},
		"./connection": {
			"default": "./dist/src/connection/index.js",
			"types": "./dist/src/connection/index.d.ts"
		},
		"./authenticate": {
			"default": "./dist/src/authenticate.js",
			"types": "./dist/src/authenticate.d.ts"
		},
		"./decode": {
			"default": "./dist/src/decode.js",
			"types": "./dist/src/decode.d.ts"
		},
		"./encode": {
			"default": "./dist/src/encode.js",
			"types": "./dist/src/encode.d.ts"
		},
		"./events": {
			"default": "./dist/src/events.js",
			"types": "./dist/src/events.d.ts"
		},
		"./factory": {
			"default": "./dist/src/factory.js",
			"types": "./dist/src/factory.d.ts"
		},
		"./layouts": {
			"default": "./dist/src/layouts/index.js",
			"types": "./dist/src/layouts/index.d.ts"
		},
		"./admin": "./src/layouts/Admin.astro",
		"./aside": "./src/layouts/Aside.vue"
	},
	"types": "./dist/clubs-core.d.ts",
	"files": [
		"dist/**/*.d.ts",
		"dist/**/*.js",
		"!**/*.test.*",
		"src"
	],
	"scripts": {
		"test": "ava",
		"preview": "run-p preview:tsc preview:rollup preview:astro",
		"preview:astro": "astro --root ui-preview/ dev",
		"preview:tsc": "yarn build:ts --watch",
		"preview:rollup": "yarn build:rollup:ui -w",
		"build": "yarn build:ts && yarn build:rollup:ui && yarn build:rollup",
		"build:ts": "tsc -p tsconfig.build.json",
		"build:rollup": "rollup -c",
		"build:rollup:ui": "rollup -c rollup.config.ui.js",
		"watch": "tsc -w",
		"pretest": "yarn prebuild && tsc -p tsconfig.test.json",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,mts,js,mjs,json,md,yml,astro}'",
		"prepack": "yarn test && yarn build",
		"prepare": "husky install"
	},
	"author": "aggre",
	"license": "MIT",
	"dependencies": {
		"@devprotocol/dev-kit": "6.10.0",
		"@devprotocol/elements": "0.2.2",
		"@devprotocol/util-ts": "2.2.1",
		"@metamask/detect-provider": "^2.0.0",
		"ethers": "^5.7.0",
		"js-base64": "^3.7.2",
		"lit": "^2.3.1",
		"ramda": "^0.28.0",
		"truncate-eth-address": "^1.0.2",
		"web3modal": "^1.9.9",
		"yaml": "^2.1.1"
	},
	"devDependencies": {
		"@astrojs/lit": "1.0.1",
		"@astrojs/react": "1.2.2",
		"@astrojs/svelte": "1.0.2",
		"@astrojs/tailwind": "2.1.3",
		"@astrojs/vue": "1.2.1",
		"@ava/typescript": "3.0.1",
		"@devprotocol/hashi": "1.4.0",
		"@rollup/plugin-commonjs": "23.0.3",
		"@rollup/plugin-multi-entry": "6.0.0",
		"@rollup/plugin-typescript": "10.0.1",
		"@rollup/pluginutils": "5.0.2",
		"@types/get-installed-path": "4.0.1",
		"@types/node": "18.11.11",
		"@types/ramda": "0.28.20",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.9",
		"@typescript-eslint/eslint-plugin": "5.45.1",
		"@typescript-eslint/parser": "5.45.1",
		"@webcomponents/template-shadowroot": "0.1.0",
		"astro": "1.6.13",
		"autoprefixer": "10.4.13",
		"ava": "5.1.0",
		"eslint": "8.29.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-functional": "4.4.1",
		"glob": "8.0.3",
		"husky": "8.0.2",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.19",
		"prettier": "2.8.0",
		"prettier-plugin-astro": "0.7.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rimraf": "3.0.2",
		"rollup": "3.6.0",
		"rollup-plugin-dts": "5.0.0",
		"sass": "1.56.1",
		"svelte": "3.54.0",
		"tailwindcss": "3.2.4",
		"typescript": "4.9.3",
		"vue": "3.2.45"
	},
	"peerDependencies": {
		"astro": "^1.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"svelte": "^3.49.0",
		"vue": "^3.2.0"
	},
	"optionalDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"svelte": "^3.49.0",
		"vue": "^3.2.0"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dev-protocol/clubs-core.git"
	},
	"keywords": [],
	"bugs": {
		"url": "https://github.com/dev-protocol/clubs-core/issues"
	},
	"homepage": "https://github.com/dev-protocol/clubs-core#readme"
}
