---
import Aside from './Aside.vue'
import type { ClubsConfiguration } from '../types'

const { config, showConnectButton } = Astro.props as {
	config: ClubsConfiguration
	showConnectButton?: boolean
}
---

<Aside config={config} showConnectButton={showConnectButton} client:only="vue">
	<slot name="aside:connect-button" slot="connect-button" />

	<slot />
</Aside>
