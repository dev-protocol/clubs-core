	config: () =>
		encode({
			name: 'Debug',
			twitterHandle: '@debug',
			description: '',
			url: '',
			propertyAddress: '',
			plugins: [{ name: 'example', options: [] }],
		}),
	plugins: { example },
})

const Content = Astro.props.component

console.log(Astro.props)
